{
    "name": "tic-tac-toe-app",
    "version": "1.0.0",
    "description": "Tri-platform (iOS, android and web) online multiplayer Tic Tac Toe game, built using React Native, Expo and Firebase.",
    "main": "index.js",
    "scripts": {
        "start": "cd client && expo start",
        "start-c": "cd client && expo start -c",
        "ios": "cd client && expo start --ios",
        "android": "cd client && expo start --android",
        "web": "cd web && npm run dev",
        "build:ios": "cd client && expo build:ios",
        "build:android": "cd client && expo build:android",
        "deploy-server": "cd server && npm run deploy",
        "deploy-landing": "cd landing-page && npm run build && firebase deploy --only hosting"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/andordavoti/tic-tac-toe-app.git"
    },
    "keywords": [],
