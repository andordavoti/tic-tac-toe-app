{
    "expo": {
        "name": "Tic Tac Toe",
        "description": "Online Tic Tac Toe game for mobile and web.",
        "githubUrl": "https://github.com/andordavoti/tic-tac-toe-app",
        "slug": "tic-tac-toe",
        "privacy": "public",
        "userInterfaceStyle": "automatic",
        "platforms": ["ios", "android", "web"],
        "version": "1.0.4",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0,
            "enabled": false
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "bundleIdentifier": "com.andordavoti.tictactoe.game",
            "buildNumber": "2023052701",
            "supportsTablet": false,
            "isTabletOnly": false,
            "config": {
                "usesNonExemptEncryption": false
            },
            "appStoreUrl": "https://apps.apple.com/us/app/tic-tac-toe-online/id1513609441",
            "googleServicesFile": "./GoogleService-Info.plist"
        },
        "android": {
            "package": "com.andordavoti.tictactoe.game",
            "versionCode": 2023052701,
            "permissions": [],
            "playStoreUrl": "https://play.google.com/store/apps/details?id=com.andordavoti.tictactoe.game",
            "googleServicesFile": "./google-services.json",
            "blockedPermissions": [
                "android.permission.REQUEST_INSTALL_PACKAGES",
                "android.permission.WRITE_EXTERNAL_STORAGE"
            ]
        },
        "web": {
            "favicon": "./assets/icon.png"
        },
        "plugins": [
            "@react-native-firebase/app",
            "./config-plugins/rnf-disable-ad-id-support.plugin.js",
            [
                "expo-build-properties",
                {
                    "ios": {
                        "useFrameworks": "static"
                    }
                }
            ]
        ]
    }
}
