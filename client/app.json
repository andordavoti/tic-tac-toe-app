{
    "expo": {
        "name": "Tic Tac Toe",
        "description": "Online Tic Tac Toe game for mobile and web.",
        "githubUrl": "https://github.com/andordavoti/tic-tac-toe-app",
        "slug": "tic-tac-toe",
        "privacy": "public",
        "userInterfaceStyle": "automatic",
        "platforms": ["ios", "android", "web"],
        "version": "1.0.3",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0,
            "enabled": false
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "bundleIdentifier": "com.andordavoti.tictactoe.game",
            "buildNumber": "2020062601",
            "supportsTablet": false,
            "isTabletOnly": false,
            "config": {
                "usesNonExemptEncryption": false
            },
            "appStoreUrl": "https://apps.apple.com/us/app/tic-tac-toe-online/id1513609441",
            "googleServicesFile": "./GoogleService-Info.plist"
        },
        "android": {
            "package": "com.andordavoti.tictactoe.game",
            "versionCode": 2020062601,
            "permissions": [],
            "playStoreUrl": "https://play.google.com/store/apps/details?id=com.andordavoti.tictactoe.game",
            "googleServicesFile": "./google-services.json"
        },
        "web": {
            "favicon": "./assets/icon.png"
        },
        "plugins": [
            // "sentry-expo",
            "@react-native-firebase/app",
            [
                "expo-build-properties",
                {
                    "ios": {
                        "useFrameworks": "static"
                    }
                }
            ]
        ]
    }
}
